{
  "name": "@final-env/babel",
  "version": "0.1.9",
  "description": "Load environment variables into __ENV__ object",
  "license": "MIT",
  "author": "Ernest",
  "files": [
    "dist"
  ],
  "keywords": [
    "babel-plugin"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist bin && pnpm build-bundle",
    "build-bundle": "esbuild src/index.ts --bundle --platform=node --target=node12 --external:vite --external:dotenv --outfile=dist/index.js",
    "release": "standard-version -t babel --preset=conventionalcommits --releaseCommitMessageFormat 'chore(release): @final-env/babel@{{currentTag}}' --path . && pnpm build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iendeavor/final-env.git",
    "directory": "packages/babel"
  },
  "bugs": {
    "url": "https://github.com/iendeavor/final-env/issues"
  },
  "homepage": "https://github.com/iendeavor/final-env/tree/main/packages/babel#readme",
  "peerDependencies": {
    "dotenv": "^11.0.0 || ^12.0.4 || ^13.0.1 || ^14.3.2 || ^15.0.1 || ^16.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.19.3",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/babel-plugin-tester": "^9.0.5",
    "@types/babel__core": "^7.1.19",
    "babel-plugin-tester": "^10.1.0",
    "dotenv": "^11.0.0 || ^12.0.4 || ^13.0.1 || ^14.3.2 || ^15.0.1 || ^16.0.0"
  }
}
